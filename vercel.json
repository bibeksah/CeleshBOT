{
    "version": 2,
    "functions": {
      "api/chat.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "routes": [
      {
        "src": "/answer",
        "dest": "/api/answer.js"
      },
      {
        "src": "/conclusion",
        "dest": "/api/chatconcludor.js"
      },
      {
        "src": "/enhance",
        "dest": "/api/chatenhancer.js"
      },
      {
        "src": "/chunkprocessor",
        "dest": "/api/chunkprocessor.js"
      }
    ]
  }